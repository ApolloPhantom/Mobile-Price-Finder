{
  "version": 2,
  "routes": [
    { "src": "/(.*)", "dest": "/app.py" }
  ],
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "env": {
    "FLASK_ENV": "production"
  },
  "functions": {
    "api": {
      "runtime": "python3.8",
      "memory": 1024
    }
  }
}
